{
  "name": "fsjs-pair-project-phase-1",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:watch": "nodemon ./bin/www",
    "start:debug": "DEBUG=express:* node --inspect ./bin/www",
    "db:init": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:fresh": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "format": "prettier --write ."
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "debug": "~2.6.9",
    "ejs": "^3.1.10",
    "express": "^4.20.0",
    "express-session": "^1.18.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "prettier": "3.3.3",
    "sequelize-cli": "^6.6.2"
  }
}
