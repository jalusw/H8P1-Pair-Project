<% if(messages.success) { %>
  <script>
  Swal.fire({
    title: "Success !",
    text: "<%= messages.success %>",
    icon: "success"
  });
  </script>
<% } %>

<% if(messages.error) { %>
  <script>
  Swal.fire({
    title: "Failed !",
    text: "<%= messages.error %>",
    icon: "error"
  });
  </script>
<% } %>
